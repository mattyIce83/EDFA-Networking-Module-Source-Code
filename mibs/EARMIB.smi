EARMIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, Integer32, IpAddress, Unsigned32
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION, DisplayString, RowStatus
    	FROM SNMPv2-TC
    ipgProducts, ipgModules
   		FROM IPGPHOTONICS-GLOBAL-REG;
        

ipgEarMibModule MODULE-IDENTITY
     LAST-UPDATED "200702191811Z"
     ORGANIZATION "IPG Photonics"
     CONTACT-INFO 
        "Telecom Engineering
         
         Postal: 50 Old Webster Rd
                 Oxford, MA 01540
                 US
                 
         Tel:    508-373-1100             

         WEB:    www.ipgphotonics.com"
     DESCRIPTION  "EAR EDFA MIB module"
     REVISION     "200702191811Z"
     DESCRIPTION  "Rev. 1.6"
     ::= { ipgModules 2 }


-- root for items in the EDFA MIB Module
ipgEdfaMIB  OBJECT IDENTIFIER ::= {ipgProducts 1}                                                      

ipgEdfaSys     		OBJECT IDENTIFIER ::={ipgEdfaMIB 1} 
ipgEdfaAttr     	OBJECT IDENTIFIER ::={ipgEdfaMIB 2} 
ipgEdfaAlarmSettings  	OBJECT IDENTIFIER ::={ipgEdfaMIB 3}  
ipgEdfaAlarmRaised	OBJECT IDENTIFIER ::={ipgEdfaMIB 4}      
ipgEdfaAlarmCleared	OBJECT IDENTIFIER ::={ipgEdfaMIB 5}      
ipgEdfaEventRaised	OBJECT IDENTIFIER ::={ipgEdfaMIB 6} 
ipgEdfaHistory		OBJECT IDENTIFIER ::={ipgEdfaMIB 7} 


-- textual conventions
TenthdB ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"This unit is used to represent expressions that are 
		 normally expressed in Decibels (dB).  For example, 
		  10.1 dB is represented as 101 dB."
	SYNTAX		Integer32(-100..400)	
	
TenthdBm ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"This unit is used to represent expressions that are 
		 normally expressed in decibels (dBm).  The m refers to 
		 the milliWatt reference level used.  For example, 10.1 dBm is represented 
		 as 101 dBm."
	SYNTAX		Integer32(-100..400)	 

MilliWatts ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"This unit is used to represent expressions that are 
		 normally expressed in milliwatts (mW)."
	SYNTAX		Integer32(0..10)	 

EarAlarmTimestamp ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"Time that alarm occurred"
	SYNTAX OCTET STRING (SIZE (50))

EarEventTimestamp ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"Time that event occurred"
	SYNTAX OCTET STRING (SIZE (50))

EarHistoryTimestamp ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"Time that the historical event occurred"
	SYNTAX OCTET STRING (SIZE (50))

EarEnabled  ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION 
                "Indicates disabled(0) or enabled(1)"
        SYNTAX INTEGER{
                disabled (0),
                enabled (1)
        }

EarEventLevel ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"Event severity:  major(1) minor(2) none(3)"
	SYNTAX INTEGER{
		major	(1),
		minor	(2),
		none	(3)
	}
	
EarAlarmIndex ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"Index of a row in the alarm status table."
	SYNTAX Integer32(1..13)
	
EarSnmpMngrIndex ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"Index of a row in the SNMP managers table."
	SYNTAX Integer32(1..6)
	
EarEventIndex ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION        
		"Index of a row in the event status table."
	SYNTAX Integer32(1..100)

EarDeviceType ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION
            "Device type legacy(1), master(2), current(3)"
        SYNTAX INTEGER{
                legacy          (1),
                master          (2),
                slave           (3)
        }
	
EarDataMode ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION
            "ALS on/off or Nominal"
        SYNTAX INTEGER{
                off             (1),
                als             (2),
                nominal         (3)
        }

EarControlMode ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION
            "Control algorithm:  power(1)/current(2)/gain(3)"
        SYNTAX INTEGER {
                power           (1),
                current         (2),
                gain            (3)
        }

EarDigiSelectedFirmwareBank ::= TEXTUAL-CONVENTION
        STATUS          current 
	DESCRIPTION
            "Firmware bank selected:  flashbank1(1) flashbank2(2)"
        SYNTAX INTEGER {
                flashbank1      (1),
                flashbank2      (2)
        }

EarDigiFirmwareConfirm ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION
            "Firmware bank confirmed:  confirmed(1) unconfirmed(2)"
        SYNTAX INTEGER {
                confirmed       (1),
                unconfirmed     (2)
        }

EarRelayStatus ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION
            "relay tripped   normal(1) tripped(2)"
        SYNTAX INTEGER {
                normal          (1),
                tripped         (2)
        }

