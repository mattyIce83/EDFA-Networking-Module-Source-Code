-- Table for description and alarm set actions
earAlarmTable OBJECT-TYPE				-- earAlarmTable is the table identifier
	SYNTAX 		SEQUENCE OF EarAlarmEntry	-- SEQUENCE of <sequence identifier>
	MAX-ACCESS  not-accessible
	STATUS		current
	DESCRIPTION
		"Table for current alarm conditions present on the system"
	::={ipgEdfaAlarmSettings 27}

earAlarmEntry OBJECT-TYPE				-- earAlarmEntry is the row identifier
	SYNTAX		EarAlarmEntry
	MAX-ACCESS  not-accessible
	STATUS		current
	DESCRIPTION
		"A row in the alarm table created when an alarm condition 
		 occurs"
	INDEX		{earAlarmIndex}
	::={earAlarmTable 1}	
	
EarAlarmEntry ::= SEQUENCE{	
	earAlarmIndex     EarAlarmIndex,
	earAlarmType      INTEGER,
	earAlarmTimeStamp EarAlarmTimestamp,
	earAlarmSeverity  EarEventLevel
	}
	
earAlarmIndex OBJECT-TYPE
	SYNTAX 		EarAlarmIndex
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"Row index for alarm status table"
	::={earAlarmEntry 1}			

earAlarmType OBJECT-TYPE
	SYNTAX INTEGER
	{
			alarmTemperatureFlag     (1),
			alarmFanFlag             (2),
			alarmOutputTooHighFlag   (3),
			alarmOutputHighFlag      (4),
			alarmOutputLowFlag       (5),
			alarmInputTooHighFlag    (6),
			alarmInputHighFlag       (7),
			alarmInputLowFlag        (8),
			alarmOutputOffFlag       (9),
			alarmInputOffFlag       (10),
			alarmInterlockFlag      (11),
			alarmPs1FailFlag        (12),
			alarmPs2FailFlag        (13),
			alarmPowerSafetyFlag    (14),
			alarmKeyswitchFlag	(15),
                        alarmOutput2OffFlag     (16),
                        alarmOutput2LowFlag     (17),
                        alarmOutput2HighFlag    (18),
                        alarmOutput2TooHighFlag (19),
                        alarmPumpFlag           (20)
	}
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	
		"One or more alarms have occurred in the system.  Signal the notification(s).
		 Alarms include the following:

			ALARM_TEMPERATURE_FLAG        (1)
			ALARM_FAN_FLAG                (2)
			ALARM_OUTPUT_TOO_HIGH_FLAG    (3)
			ALARM_OUTPUT_HIGH_FLAG        (4)
			ALARM_OUTPUT_LOW_FLAG         (5)
			ALARM_INPUT_TOO_HIGH_FLAG     (6)
			ALARM_INPUT_HIGH_FLAG         (7)
			ALARM_INPUT_LOW_FLAG          (8)
			ALARM_OUTPUT_OFF_FLAG         (9)
			ALARM_INPUT_OFF_FLAG         (10)
			ALARM_INTERLOCK_FLAG         (11)
			ALARM_PS1_FAIL_FLAG          (12)
			ALARM_PS2_FAIL_FLAG          (13)
			ALARM_POWER_SAFETY_FLAG      (14)
			ALARM_KEYSWITCH_FLAG         (15)
			ALARM_OUTPUT_2_OFF_FLAG      (16)
			ALARM_OUTPUT_2_LOW_FLAG      (17)
			ALARM_OUTPUT_2_HIGH_FLAG     (18)
			ALARM_OUTPUT_2_TOO_HIGH_FLAG (19)
			ALARM_PUMP_FLAG              (20)"
	::={earAlarmEntry 2}			

earAlarmTimeStamp OBJECT-TYPE
	SYNTAX		EarAlarmTimestamp
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	
		"The time the alarm was triggered"
	::={earAlarmEntry 3}			

earAlarmSeverity OBJECT-TYPE
	SYNTAX EarEventLevel
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	
		"Severity of the triggered alarm"
	::={earAlarmEntry 4}


