-- the SNMP Management Station address table

 earSnmpMngrTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF EarSnmpMngrEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
             "The table of SNMP network managers."
     ::= { ipgEdfaSys 21 }

 earSnmpMngrEntry OBJECT-TYPE
     SYNTAX      EarSnmpMngrEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
             "The information for one SNMP network manager."
     INDEX      { earSnmpMngrIndex }
     ::= { earSnmpMngrTable 1 }

 EarSnmpMngrEntry ::= SEQUENCE {
         earSnmpMngrIndex     EarSnmpMngrIndex,
         earSnmpMngrIpAddr    IpAddress,
         earSnmpMngrRowStatus RowStatus
     }

